  <string id="str_multiplayer_official_game_type_explainer.cRPGConquest" text="{=JLGDB5T7}·   Capture all flags of a stage to advance to the next one{newline}·   Defenders' reward multipler increases over time and is reset on a new stage{newline}·   Attackers' reward multiplier decreases over time and is set to max on a new stage" />
  <string id="str_multiplayer_game_type.cRPGConquest" text="{=PhKxWoxz}Conquest" />

  <!-- Siege -->
  <string id="str_multiplayer_official_game_type_name.cRPGSiege" text="{=uMbyjZKn}Siege" />
  <string id="str_multiplayer_official_game_type_explainer.cRPGSiege" text="{=NyzhLxjg}·   Use various siege engines to attack or defend control points{newline}·   Spawn points advance as control points are captured{newline}·   Successfully assault and hold the keep flag or repel the enemy to win" />
  <string id="str_multiplayer_game_type.cRPGSiege" text="{=6NrVsGO5}Siege" />

  <!-- Team Deathmatch -->
  <string id="str_multiplayer_official_game_type_name.cRPGTeamDeathmatch" text="{=JWb4DWDq}Team Deathmatch" />
